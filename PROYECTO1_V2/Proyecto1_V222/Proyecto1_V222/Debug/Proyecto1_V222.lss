                                 CONVERTIR_UD:
                                     ; -------------------- CONVERTIR contador_minutos --------------------
0000f4 9100 0102                     LDS R16, contador_minutos    ; Cargar valor del contador de minutos
0000f6 940e 0125                     CALL DIVIDIR_10
